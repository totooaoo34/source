		<property name="comicIma01Url"></property>
		<property name="comicIma02Url"></property>
		<property name="comicIma03Url"></property>
		<property name="comicIma04Url"></property>
		<property name="comicIma05Url"></property>
		<property name="comicIma06Url"></property>
		<property name="comicIma07Url"></property>
		<property name="comicIma08Url"></property>
