<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.entity">
	<class name="Ai_student">
		<id name="S_id">
			<generator class="sequence">
				<param name="sequence">eq_ai_student</param>
			</generator>
		</id>

		<property name="S_name" ></property>
		<property name="S_sex" ></property>
		<property name="S_age" ></property>
		<property name="S_phone" ></property>
		<property name="S_address" ></property>
		<property name="S_indate" ></property>
		<property name="S_outdate" ></property>
		<property name="S_image" ></property>
		<!-- name：在这个类中关联属性的变量名 -->
		<!-- property-ref：对方(Wife类)用来关联我的变量名
		<one-to-one name="wife" property-ref="husband"></one-to-one> -->

	</class>
</hibernate-mapping>
