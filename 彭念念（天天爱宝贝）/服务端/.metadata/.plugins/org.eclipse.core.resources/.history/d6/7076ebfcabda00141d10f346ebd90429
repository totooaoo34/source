package com.dao;

import java.util.List;

import org.springframework.orm.hibernate3.HibernateTemplate;

import com.entity.Chengshi_1;

public class Chenshi_Dao {
	private HibernateTemplate template;

	public void setTemplate(HibernateTemplate template) {
		this.template = template;
	}
	
	//查询城市1
	public List<Chengshi_1> findAll_Chengshi_1(){
		List<Chengshi_1> loadAll = template.loadAll(Chengshi_1.class);
		return loadAll;
	}
	
	//查询城市2
	public List<Chengshi_1> findAll_Chengshi_2(){
		List<Chengshi_1> loadAll = template.loadAll(Chengshi_1.class);
		return loadAll;
	}
	//查询城市3
	public List<Chengshi_1> findAll_Chengshi_3(){
		List<Chengshi_1> loadAll = template.loadAll(Chengshi_1.class);
		return loadAll;
	}
}
