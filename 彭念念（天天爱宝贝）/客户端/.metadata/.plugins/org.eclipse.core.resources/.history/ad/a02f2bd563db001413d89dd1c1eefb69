	private List<Chengshi_2> list2;
	private List<Chengshi_3> lis3;
	
	public MychengshiAdapter(Context cn, List<Chengshi_1> list1,
			List<Chengshi_2> list2, List<Chengshi_3> lis3) {
		super();
		this.cn = cn;
		this.list1 = list1;
		this.list2 = list2;
		this.lis3 = lis3;
	}

	@Override
	public int getCount() {
		// TODO Auto-generated method stub
		if(list1!=null){
			return list1.size();
		}
		if(list2!=null){
			return list2.size();
		}
		if(list3!=null){
			return list3.size();
		}
		
	}

